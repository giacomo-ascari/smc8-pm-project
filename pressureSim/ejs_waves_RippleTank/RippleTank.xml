public void saveMyState () {
  studentMode=saveInStudentMode;
  _updateModel();
  _view.update();
  if(OSPRuntime.isLauncherMode() || OSPRuntime.isAppletMode() || _simulation.isMoodleConnected()) {
    _saveState(null);
  } else {
     if(fileName==null)fileName="";
     fileName=fileName.trim();
    _saveDefaultStateToJar(fileName);
  }
}
]]></Code>
