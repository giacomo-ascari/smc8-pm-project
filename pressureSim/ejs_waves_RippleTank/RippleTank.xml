<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Compute Wave</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
_showDescriptionAtStartUp(!studentMode);
// wave superposition is computed at each grid point
if(n!=amp.length) {
  initializeSource();
}
double PI2 = Math.PI*2;
double cos = Math.cos(-PI2*t);
double sin = Math.sin(-PI2*t);
for(int i=0; i<m; i++) {                         // loop over grid x
  double xi= _view.scalarField.indexToX(i);      // x-coordinate of grid point
  for(int j=0; j<m; j++) {                       // loop over grid y
    double yj= _view.scalarField.indexToY(j);    // y-x-coordinate of grid point
    wavePattern[i][j]=0;                           // zero potential value
    for(int p=0; p<n; p++) {                     // loop over sources
      double dx=xi-x[p];                         // x-separation from grid point
      double dy=yj-y[p];                         // y-separation from grid point
      double r=Math.sqrt(dx*dx+dy*dy)/wavelength;// distance in terms of wavelength
      if(r!=0) {   // check for singularity
        double rad=Math.toRadians(phi[p]);
        double fac=1;
        if(power==1)fac=1.0/Math.sqrt(r);
        else if(power==2)fac=1.0/r; 
        double re=Math.cos(PI2*r+rad)*fac;
        double im=Math.sin(PI2*r+rad)*fac;
        double s=cos*re-sin*im;
        wavePattern[i][j] += s;
      } // end of if statement
    } // end of source loop
    if(energyDensity)wavePattern[i][j]=wavePattern[i][j]*wavePattern[i][j];
  } // end of y loop
} // end of x loop
]]></Code>
</Content>
</Osejs.Model.Constraints.Page>
